@using doan_dbsm.Models.DATA

<div class="wrap-icon-header flex-w flex-r-m">
    @if (Session["taikhoan"] != null && Session["taikhoan"] != "")
    {

        <li> @Session["taikhoan"]</li>
        <div class="icon-header-item cl0 hov-cl1 trans-04 p-lr-11 " data-notify="2">
            <a href="@Url.Action("dangxuat","Login")" class="text-dark">
                @*<i class="zmdi zmdi-account"></i>*@
                <span class="iconify mt-1" data-icon="mdi:account-arrow-right" data-inline="false"></span>

            </a>

        </div>

    }
    else
    {
        <div class="icon-header-item cl0 hov-cl1 trans-04 p-lr-11 " data-notify="2">
            <a href="@Url.Action("Index","Login")" class="text-dark">
                <i class="zmdi zmdi-account"></i>

            </a>

        </div>
    }

    <div class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 icon-header-noti "
         data-notify=" @ViewBag.Tongsoluong">
        <a href="@Url.Action("Cart","CART")" class="text-dark">
            <i class="zmdi zmdi-shopping-cart"></i>
        </a>
    </div>


    @*<div class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 icon-header-noti js-show-cart"
             data-notify="2">
            <i class="zmdi zmdi-shopping-cart"></i>
        </div>*@

    <a href="#"
       class="dis-block icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 icon-header-noti"
       data-notify="0">
        <i class="zmdi zmdi-favorite-outline"></i>
    </a>
</div>
<script src="https://code.iconify.design/1/1.0.3/iconify.min.js"></script>

